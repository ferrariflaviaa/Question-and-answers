<%- include('partials/header') %>

  <body>
    <%- include('partials/navbar') %>
    <div class="container">
      <hr>
      <h2>Perguntas</h2>
      <hr>
      <a href="/perguntar" class="btn btn-dark">Perguntar</a>
      <hr>
      <% perguntas.forEach(pergunta=> {%>
        <br>
        <div class="card">
          <div class="card-body">
            <p>
              <%= pergunta.titulo%>
            </p>
          </div>
          <div class="card-footer">
           <button class="btn btn-dark">Responder</button>
          </div>
        </div>
        <%})%>
    </div>
  </body>
  <%- include('partials/footer') %>